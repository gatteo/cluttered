{
  "name": "cluttered",
  "version": "1.0.0",
  "description": "Reclaim disk space by cleaning dev project artifacts - node_modules, build caches, and more",
  "main": "dist/main/main/index.js",
  "author": "Matteo Giardino <me@matteogiardino.com>",
  "license": "MIT",
  "homepage": "https://cluttered.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatteo/cluttered.git"
  },
  "bugs": {
    "url": "https://github.com/gatteo/cluttered/issues"
  },
  "keywords": [
    "disk-cleaner",
    "developer-tools",
    "node_modules",
    "disk-space",
    "macos",
    "electron",
    "cleanup",
    "clean my mac"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:renderer": "vite",
    "dev:main": "npm run build:main && electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "package": "npm run build && electron-builder",
    "package:dir": "npm run build && electron-builder --dir",
    "package:mas": "./scripts/build-mas.sh",
    "package:dmg": "./scripts/build-dmg.sh",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist release",
    "postinstall": "electron-rebuild"
  },
  "dependencies": {
    "better-sqlite3": "^11.6.0",
    "check-disk-space": "^3.4.0",
    "dockerode": "^4.0.2",
    "electron-store": "^8.2.0",
    "framer-motion": "^11.15.0",
    "geist": "^1.5.1",
    "glob": "^11.0.0",
    "lucide-react": "^0.562.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "simple-git": "^3.30.0",
    "uuid": "^11.0.3",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/dockerode": "^3.3.31",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.5"
  }
}
